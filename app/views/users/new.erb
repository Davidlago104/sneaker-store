<div class="h1">

<h1>Sign up here!</h1>

<%= render partial: 'layouts/error', locals: {thing: @user} %>

<%= form_for(@user, url: signup_path(@user)) do |f|%>
  <%=f.label :username %>
  <%=f.text_field :username%>
  <br>
  <br>
  <%= f.label :password%>
  <%=f.password_field :password%>
  <br>
  <br>
  <%=f.submit "submit"%>

<% end %>
</div>
