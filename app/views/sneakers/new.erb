<h1>Add a sneaker!</h1>

<%= form_for(:sneaker) do |f|%>
  <%= f.label :name%>
  <%=f.text_field :name%>

  <%= f.label :price%>
  <%= f.number_field :price%>
  <br>
  <%=f.label :description%>
  <%=f.text_field :description%>
  <br>
  <p>
    Select a brand
  </p>

  <p>
    add a new brand
  </p>
  
  <br>
  <%= f.submit "Log in!"%>
<% end %>
